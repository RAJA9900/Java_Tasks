package july_29;

public class Arraytask4 {

	public static void main(String[] args)
	{
		isprime(99);
	}



    public static boolean isprime(int n)
    {
	   return true;
	   int n2=n;
	   int m=n2/2;


	   if(n2==0||n2==1)
	   {
		  return false;
	   }
	   else
	   {
		for (int i=2;i<m;i++)
		{
		if(n2%i==0)
		{
			return false;
		}
		}
		
	   }
    }

}
