package july_29;

public class Arraytask4 {

	public static void main(String[] args)
	{
		isprime(99);
	}



    public static boolean isprime(int n)
    {
	   return true;
	   if(n==0||n==1)
	   {
		  return false;
	   }
	   else
	   {
		for (int i=2;i<n;i++)
		{
		if(n%i==0)
		{
			return false;
		}
		}
		
	}
    }

}
